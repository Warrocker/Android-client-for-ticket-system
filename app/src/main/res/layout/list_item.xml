<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#ffffff">
    <TextView
        android:id="@+id/ticketid"
        android:layout_width="fill_parent"
        android:paddingTop="1dip"
        android:layout_height="wrap_content"
        android:textStyle="bold"
        android:textColor="#000000"
        android:layout_alignParentTop="true"
        android:layout_alignEnd="@+id/performer" />
    <TextView
        android:id="@+id/title"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:paddingTop="6dip"
        android:textColor="#000000"
        android:textStyle="bold"
        android:layout_below="@+id/ticketid"
        android:layout_alignEnd="@+id/performer" />
    <TextView
        android:id="@+id/st_dtime"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:paddingTop="6dip"
        android:textStyle="bold"
        android:textColor="#000000"
        android:layout_below="@+id/title"
        android:layout_alignEnd="@+id/performer" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/performer"
        android:height="20dp"
        android:layout_alignParentEnd="true"
        android:layout_below="@+id/priorityView"
        android:layout_alignParentStart="true"
        android:textColor="#000000"/>
    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/priorityView"
        android:layout_below="@+id/st_dtime"
        android:layout_alignParentStart="true"
        android:layout_alignEnd="@+id/ticketid" />
</RelativeLayout>